package org.joonzis.test;

class Human{
	String name;
	int age;
	
	public Human(String name, int age) {
		super();
		this.name = name;
		this.age = age;
	}
	
	void output() {
		System.out.println("이름 : " + name + "," + "나이 : " + age);
	}
}

class Student extends Human{
	String school;
	
	public Student(String name, int age,String school) {
		super(name, age);
		this.school = school;
		// TODO 자동 생성된 생성자 스텁
	}

	@Override
	void output() {
		System.out.println("학교 : " + school);
		super.output();
	}
}

class Wocker extends Human{
	String job;

	public Wocker(String name, int age, String job) {
		super(name, age);
		this.job = job;
	}

	@Override
	void output() {
		System.out.println("직장 : " + job);
		// TODO 자동 생성된 메소드 스텁
		super.output();
	}
	
	

}

