package org.joonzis.ex;

public class Test_Oper {
	
	double val1, val2;
	
	void plus(double val1, double val2) {
		this.val1 = val1;
		this.val2 = val2;
		System.out.println(val1 + val2);
	}
	void minus() {
		val1 = 9;
		val2 = 2;
		System.out.println(val1 - val2);		
	}
	void multi() {
		val1 = 10;
		val2 = 2;
		System.out.println(val1 * val2);
		
	}
	double div() {
		val1 = 9;
		val2 = 4;
		return val1 / val2;
	}

}
