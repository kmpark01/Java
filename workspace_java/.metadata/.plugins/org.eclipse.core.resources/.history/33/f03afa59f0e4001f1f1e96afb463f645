package org.joonzis.ex;

public class Ex01_Inheritance {
	
	//부모 클래스
	class Parent{
		
		int number;
		
		void doParent() {
			System.out.println(("doParent() 호출"));
		}
	}
	
	//자식 클래스
	class Child extends Parent{
		
		void doChild() {
			System.out.println("doChild() 호출");
		}
		
	}

	public static void main(String[] args) {
		// TODO 자동 생성된 메소드 스텁

	}

}
