package org.joonzis.test;

public class RecursiveCall {
	
	static int a;
	
	static void recursive(){
		System.out.println("Hello");
		a++;
		
		if(a == 10) {
			return;
		}else {
			recursive();
		}
	}

	public static void main(String[] args) {
		recursive();
		// TODO 자동 생성된 메소드 스텁

	}

}
