package org.joonzis.test;

public class CoordinateCircleRIngMain {

	public static void main(String[] args) {
		
		Coordinate c1 = new Coordinate(3,7);
		Coordinate c2 = new Coordinate(2,8);
		
		Circle innerCircle = new Circle(2.0, c2);
		Circle outerCircle = new Circle(7.0, c2);
		
		Ring ring = new Ring(innerCircle, outerCircle);
		
		ring.output();
		// TODO 자동 생성된 메소드 스텁

	}

}
