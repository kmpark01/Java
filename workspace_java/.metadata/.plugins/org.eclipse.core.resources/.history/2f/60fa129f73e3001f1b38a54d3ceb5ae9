package org.joonzis.test;

public class RecursiveCall {
	
	static int count;
	
	static void recursive(int a){
		System.out.println("Hello");
		count++;
		
		if(a == 10) {
			return;
		}else {
			recursive(a);
		}
	}

	public static void main(String[] args) {
		recursive(5);
		// TODO 자동 생성된 메소드 스텁

	}

}
