package org.joonzis.ex;

public class Test_Oper {
	
	double val1, val2;
	
	void plus(double val1, double val2) {
		this.val1 = val1;
		this.val2 = val2;
		
		System.out.println(val1 + val2);
	}
	void minus(double val1, double val2) {
		this.val1 = val1;
		this.val2 = val2;
		System.out.println(val1 - val2);		
	}
	void multi(double val1, double val2) {
		this.val1 = val1;
		this.val2 = val2;
		System.out.println(val1 * val2);
		
	}
	void div(double val1, double val2) {
		this.val1 = val1;
		this.val2 = val2;		
		System.out.println(val1 / val2);
	}

}
