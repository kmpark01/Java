package org.joonzis.test;

public class Singer {
	
	String name;
	
	Song song = new Song();
	
	public Singer() {
		name = "순순희";
		// TODO 자동 생성된 생성자 스텁
	}
	

	
	
	public Singer(String name) {
		super();
		this.name = name;
		//this.song = song;
	}

	void setSong() {
		
		this.song = new Song("천상연", "한국");
	}
	
	void output() {
		System.out.println("제목 :" + song.title);
		System.out.println("나라 :" + song.country);
		System.out.println("가수 :" + name);
	} 

}
