package org.joonzis.test;

public class Singer {
	
	String name;
	
	Song song = new Song();
	
	public Singer() {
		name = "순순희";
		// TODO 자동 생성된 생성자 스텁
	}
	
	public Singer(String name, Song song) {
		super();
		this.name = name;
		this.song.title = song.title;
		this.song.country = song.country;
	}

	void setSong() {
		
	}
	
	void output() {
		System.out.println("제목 :" + song.title);
		System.out.println("나라 :" + song.country);
		System.out.println("가수 :" + name);
	} 

}
