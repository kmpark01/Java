package org.joonzis.test;

import java.util.Scanner;

public class Song {
	
	String title, country;
	
	Scanner sc = new Scanner(System.in);

	public Song(String title, String country) {
		super();
		
		System.out.println("노래 제목을 입력하세요 >>");
		title = sc.nextLine();
		this.title = title;
		this.country = country;
	}

}
