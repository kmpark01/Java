package org.joonzis.test;

public class StudentGradeMain {
	public static void main(String[] args) {
		
		Student std = new Student();
		std.output();
		
		System.out.println("---------------------");
		
		
		Student std2 = new Student("김", "전자공학과", null);
		std2.output();

		System.out.println("---------------------");
		
		Student std3 = new Student("이", "경제학과", null);
		std3.output();
	}

}
